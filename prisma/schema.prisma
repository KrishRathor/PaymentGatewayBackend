generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URI")
}

model Merchant {
    id Int @id @default(autoincrement())
    createdAt DateTime @default(now())
    name String @db.VarChar(255)
    publicKey String
    email String
    contact BigInt
    verified Boolean @default(false)
    apiKey String
}